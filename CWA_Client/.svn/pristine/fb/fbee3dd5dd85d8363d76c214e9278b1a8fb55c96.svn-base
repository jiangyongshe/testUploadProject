/*发布求购信息*/
.qgxx-lc-a, .qgxx-lc-b, .qgxx-lc-c, .qgxx-lc-d {
	display: inline-block;
	vertical-align: middle;
	padding-left: 3px;
	margin-left: -3px;
}
.qgxx-lc-a {
	width: 16%;
}
.qgxx-lc-a1 {
	position: relative;
	width: 100px;
	height: 100px;
	padding: 25px 8px 0;
	margin-left: 15px;
	border-radius: 100%;
	font-size: 18px;
	line-height: 27px;
	color: #fff;
	background-color: #f22e00;
}
.qgxx-lc-a1:after {
	content: "";
	position: absolute;
	right: -7px;
	top: 50%;
	margin-top: -8px;
	border-top: 8px solid transparent;
	border-bottom: 8px solid transparent;
	border-left: 8px solid #f22e00;
}
.qgxx-lc-b {
	width: 28%;
}
.qgxx-lc-b1, .qgxx-lc-b2 {
	display: inline-block;
	vertical-align: middle;
	padding-left: 3px;
	margin-left: -3px;
}
.qgxx-lc-b1 {
	position: relative;
	width: 16%;
	padding: 40px 0;
	font-size: 42px;
	font-style: italic;
}
.qgxx-lc-b1:after {
	content: "";
	position: absolute;
	right: -1px;
	top: 0;
	bottom: 0;
	border-right: 1px solid #eee;
	transform:rotate(9deg);
}
.qgxx-lc-b2 {
	width: 75%;
	padding-left: 8%;
}
.qgxx-lc-b2 h4 {
	height: 38px;
}
