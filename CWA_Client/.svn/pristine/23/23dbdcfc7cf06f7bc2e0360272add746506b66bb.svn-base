<div class="neiye bg-f9 bor-t">
  <div class="mar-auto pos-r">
    <!--左侧菜单-->
    <script type="text/javascript" src="/<spring:message code="url.language"/>/js/customerMenu.js"></script>

   <!--右侧内容-->
    <div class="right bg-ff">
      <h2 class="clear font18 pad-b10 bor-b">翔云宝</h2>
      
      <p class="font14 pad-t30">
        <span class="dis-in">账户余额：<span class="font20 yellow amount">0.00</span> 元</span>
        
        <span class="dis-in pad-l20 mar-l15">
