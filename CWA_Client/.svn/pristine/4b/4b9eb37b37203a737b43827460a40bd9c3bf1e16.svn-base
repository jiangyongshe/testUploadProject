	public String getIdle_time() {
		return idle_time;
	}
	public void setIdle_time(String idle_time) {
		if(!RegUtil.getUtil().isNull(idle_time)&&idle_time.indexOf("-")>=0){
			String begin_time=idle_time.split("-")[0];
			String end_time=idle_time.split("-")[1];
			if(!RegUtil.getUtil().isNull(begin_time)&&begin_time.length()>5){
				begin_time = begin_time.substring(0,5); 
			}
			if(!RegUtil.getUtil().isNull(end_time)&&end_time.length()>5){
				end_time = end_time.substring(0,5); 
			}
			this.idle_time = begin_time+"-"+end_time; 
		}else{
			this.idle_time = idle_time;
		}
	}
	
