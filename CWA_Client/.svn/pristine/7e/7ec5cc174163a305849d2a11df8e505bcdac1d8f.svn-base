/*按钮动画*/
.btn:hover, .btn:focus {
	-webkit-animation: pulse 1s;
	animation: pulse 1s;
	box-shadow: 0 0 0 1em rgba(255, 255, 255, 0);
}
@-webkit-keyframes pulse {
 0% {
 box-shadow: 0 0 0 0 var(--hover);
}
}
@keyframes pulse {
 0% {
 box-shadow: 0 0 0 0 var(--hover);
}
}
.bg-999 {
    --hover: #eee;
}
.bg-eee {
    --hover: #999;
}
.bg-red {
    --hover: #af1e23;
}
.bg-green {
    --hover: #1aab1d;
}
.bg-blue {
    --hover: #1b94d3;
}
.bg-yellow  {
    --hover: #ffa524;
}

/*美化单选和复选框*/
.checkbox, .radio {
	cursor: pointer;
}
.checkbox i {
	font-style: normal;
	display: inline-block;
	width: .9rem;
	height: .9rem;
	text-align: center;
	line-height: .9rem;
	color: #fff;
	vertical-align: middle;
	margin: -.1rem .1rem .05rem 0;
	border: 1px solid #ccc;
	background: #fff;
}
.radio i {
	font-style: normal;
	display: inline-block;
	width: .9rem;
	height: .9rem;
	text-align: center;
	line-height: .9rem;
	color: #fff;
	vertical-align: middle;
	margin: -.1rem .1rem .05rem 0;
	border: 1px solid #ccc;
	background: #fff;
}
.h-login .radio i {
	color: #333;
	border: 2px solid #fff;
	background-color: #333;
}
.body1 .radio i {
	color: #89b3c8;
	border: 1px solid #89b3c8;
	background-color: #89b3c8;
}
input[type="checkbox"], input[type="radio"] {
	display: none;
}
input[type="radio"] + i {
	border-radius: 100%;
}
input[type="checkbox"]:checked + i, input[type="radio"]:checked + i {
	color: #fff;
	border: 1px solid #1b94d3;
	background-color: #1b94d3;
}
.h-login input[type="checkbox"]:checked + i, .h-login input[type="radio"]:checked + i {
	color: #ff0;
	border: 1px solid #fff;
}
input[type="checkbox"]:disabled + i, input[type="radio"]:disabled + i {
	border-color: #999;
}
input[type="checkbox"]:checked:disabled + i, input[type="radio"]:checked:disabled + i {
	background: #999;
}

/*加载样式*/
.loading01 {
	position: fixed;
	left: 50%;
	top: 50%;
	z-index: 9999;
	width: 11rem;
	height: 3.5rem;
	padding: 0.25rem 0.4rem;
	margin-left: -5.5rem;
	margin-top: -1.05rem;
	border-radius: 0.5rem;
	box-shadow: 0 0 0.25rem #999;
	color: #2badd7;
	background-color: rgba(12,46,81,.9);
	display: none;
}
.loading01 .loader {
	box-sizing: border-box;
	display: flex;
	flex: 0 1 auto;
	flex-direction: column;
	flex-grow: 1;
	flex-shrink: 0;
	flex-basis: 25%;
	max-width: 85%;
	height: 2rem;
	align-items: center;
	justify-content: center;
}
.ball-clip-rotate-pulse {
	position: relative;
	-webkit-transform: translateY(-0.75rem);
	-ms-transform: translateY(-0.75rem);
	transform: translateY(-0.75rem);
}
.ball-clip-rotate-pulse > div:first-child {
	height: 0.8rem;
	width: 0.8rem;
	top: 0.45rem;
	left: 0.45rem;
	-webkit-animation: scale 1s 0s cubic-bezier(.09,.57,.49,.9) infinite;
	animation: scale 1s 0s cubic-bezier(.09,.57,.49,.9) infinite;
	background-color: #666;
}
.ball-clip-rotate-pulse > div {
	-webkit-animation-fill-mode: both;
	animation-fill-mode: both;
	position: absolute;
	top: 0;
	left: 0;
	border-radius: 100%;
}
.ball-clip-rotate-pulse > div:last-child {
	position: absolute;
	width: 1.5rem;
	height: 1.5rem;
	background: transparent;
	-webkit-animation: rotate 1s 0s cubic-bezier(.09,.57,.49,.9) infinite;
	animation: rotate 1s 0s cubic-bezier(.09,.57,.49,.9) infinite;
	-webkit-animation-duration: 1s;
	animation-duration: 1s;
	border: 2px solid #2badd7;
	border-color: #2badd7 transparent #2badd7 transparent;
}
.ball-clip-rotate-pulse > div {
	-webkit-animation-fill-mode: both;
	animation-fill-mode: both;
	position: absolute;
	top: 0;
	left: 0;
	border-radius: 100%;
}
@keyframes scale {
	30% {
	-webkit-transform: scale(0.3);
	transform: scale(0.3);
    }
    100% {
	-webkit-transform: scale(1);
	transform: scale(1);
    }
}
.ball-clip-rotate-pulse {
	position: relative;
	-webkit-transform: translateY(-0.75rem);
	-ms-transform: translateY(-0.75rem);
	transform: translateY(-0.75rem);
}
.ball-clip-rotate-pulse > div {
	-webkit-animation-fill-mode: both;
	animation-fill-mode: both;
	position: absolute;
	top: 0;
	left: 0;
	border-radius: 100%;
}
.ball-clip-rotate-pulse > div:first-child {
	height: 0.8rem;
	width: 0.8rem;
	top: 0.45rem;
	left: 0.45rem;
	-webkit-animation: scale 1s 0s cubic-bezier(.09,.57,.49,.9) infinite;
	animation: scale 1s 0s cubic-bezier(.09,.57,.49,.9) infinite;
	background-color: #2badd7;
}
.ball-clip-rotate-pulse > div:last-child {
	position: absolute;
	width: 1.5rem;
	height: 1.5rem;
	background: transparent;
	-webkit-animation: rotate 1s 0s cubic-bezier(.09,.57,.49,.9) infinite;
	animation: rotate 1s 0s cubic-bezier(.09,.57,.49,.9) infinite;
	-webkit-animation-duration: 1s;
	animation-duration: 1s;
	border: 2px solid #2badd7;
    border-color: #2badd7 transparent #2badd7 transparent;
}
@keyframes rotate {
	0% {
	-webkit-transform: rotate(0deg) scale(1);
	transform: rotate(0deg) scale(1);
    }
    50% {
	-webkit-transform: rotate(180deg) scale(0.6);
	transform: rotate(180deg) scale(0.6);
    }
    100% {
	-webkit-transform: rotate(360deg) scale(1);
	transform: rotate(360deg) scale(1);
   }
}
.ball-clip-rotate-multiple {
	position: relative;
}
.ball-clip-rotate-multiple > div {
	-webkit-animation-fill-mode: both;
	animation-fill-mode: both;
	position: absolute;
	left: 0;
	top: 0;
	border-bottom-color: transparent;
	border-top-color: transparent;
	border-radius: 100%;
	height: 1.75rem;
	width: 1.75rem;
	-webkit-animation: rotate 1s 0s ease-in-out infinite;
	animation: rotate 1s 0s ease-in-out infinite;
	border: 2px solid #666;
}
.ball-clip-rotate-multiple > div:last-child {
	display: inline-block;
	top: 0.5rem;
	left: 0.5rem;
	width: 0.75rem;
	height: 0.75rem;
	-webkit-animation-duration: 0.5s;
	animation-duration: 0.5s;
	-webkit-animation-direction: reverse;
	animation-direction: reverse;
	border-color: #666 transparent #666 transparent;
}
@-webkit-keyframes ball-scale-ripple {
	0% {
	-webkit-transform: scale(0.1);
	transform: scale(0.1);
	opacity: 1;
    }
    70% {
	-webkit-transform: scale(1);
	transform: scale(1);
	opacity: 0.7;
    }
    100% {
	opacity: 0;
    }
}

