@charset "utf-8";
/* CSS Document */
@media only screen and (max-width: 1920px) {
 html,body {
 font-size: 40px;
 }
}
@media only screen and (max-width: 640px) {
 html,body {
 font-size: 40px;
 }
}
@media only screen and (max-width: 600px) {
 html,body {
 font-size: 37.3px;
 }
}
@media only screen and (max-width: 540px) {
 html,body {
 font-size: 33px;
 }
}
@media only screen and (max-width: 480px) {
 html,body {
 font-size: 28px;
 }
}
@media only screen and (max-width: 414px) {
 html,body {
 font-size: 25.875px;
 }
}
@media only screen and (max-width: 400px) {
 html,body {
 font-size: 25px;
 }
}
@media only screen and (max-width: 375px) {
 html,body {
 font-size: 23.6px;
 }
}
@media only screen and (max-width: 360px){
 html,body {
 font-size: 22.5px;
 }
}
@media only screen and (max-width: 330px) {
 html,body {
 font-size: 20px;
 }
}
* {
    -moz-box-sizing: border-box;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
}
html, body {
	overflow-x: hidden;
}
body, div, p, ul, ol, li, textarea, form, h1, h2, h3, h4, h5, dl, dt, dd, img, section, article, aside, header, footer, nav, figure, hgroup {
	margin: 0;
	padding: 0;
}
body {
	font-family: "MicroSoft YaHei", "\5FAE\8F6F\96C5\9ED1", Helvetica, serif;
	min-width: 320px;
	max-width: 640px;
	margin: 0 auto;
	line-height: .8rem;
	-webkit-text-size-adjust: none;
	color: #666;
	word-wrap: break-word;
	word-break: break-all;
	background-color: #f9f9f9;
}
input[type="button"], input[type="submit"], input[type="reset"] {
	-webkit-appearance: none;
}
textarea {
	-webkit-appearance: none;
}
input, textarea, select {
	font: .6rem/1.5 "MicroSoft YaHei", Helvetica;
	vertical-align: middle;
	outline: none;
}
header input::-webkit-input-placeholder,
.screen-list input::-webkit-input-placeholder,
.ggtf-sc3 input::-webkit-input-placeholder {
	color: #2ca6e0;
}
img {
	border: 0;
}
i, em {
	font-style: normal;
}
table {
	border-collapse: collapse;
	border-spacing: 0;
}
ul, ol, li, dl, dd {
	list-style: none;
}
a {
	color: #666;
	text-decoration: none;
}
a:hover {
	color: #2badd7;
	text-decoration: none;
}

/*头部标题*/
.header-bt {
	position: fixed;
	left: 0;
	top: 0;
	right: 0;
	z-index: 31;
	height: 1.8rem;
	padding: .5rem .6rem 0;
	background-color: rgba(12,46,81,.9);
}
.header-bt .fanhui {
	left: .5rem;
	top: .2rem;
}

/*表格样式*/
.rmgg-table {
	overflow-x: auto;
	overflow-y: hidden;
}
.rmgg-table th, .rmgg-table td {
	padding: .4rem .3rem;
	text-align: center;
	white-space: nowrap;
}
.rmgg-table th {
	background-color: #f3f3f3;
}
.rmgg-table td {
	border-bottom: 1px solid #eee;
}
.rmgg-table tr:nth-child(2n-1) {
	background-color: #f9f9f9;
}

/*弹出提示样式*/
.shade {
	position: fixed;
	left: 0;
	top: 0;
	bottom: 0;
	right: 0;
	z-index: 1000;
	font-size: .6rem;
	background: rgba(153,153,153,0.5);
	display: none;
}
.tanchu {
	position: absolute;
	left: .5rem;
	top: 50%;
	right: .5rem;
	margin-top: -7rem;
	border-radius: .3rem;
	box-shadow: 0 0 .25rem #999;
	color: #2badd7;
	background-color: rgba(12,46,81,.9);
}
.tanchu1 {
	box-shadow: 0 0 0 #999;
	background-color: rgba(153,153,153,0);
}
.tanchu1 > .tanchu1-nr > img {
	width: 100%;
}
.tanchu2 {
	height: 10rem;
	margin-top: -5rem;
}
.tanchu3 {
	height: 5rem;
	margin-top: -2.5rem;
	color: #2badd7;
	background-color: rgba(12,46,81,.9);
}
.tanchu-sp {
	position: absolute;
	left: .5rem;
	top: .5rem;
	right: .5rem;
}
.tanchu-xy {
	position: absolute;
	left: .5rem;
	top: .5rem;
	right: .5rem;
	bottom: .5rem;
	border-radius: .3rem;
	box-shadow: 0 0 .25rem #999;
	color: #666;
	background-color: #fff;
}
.tanchu-bt {
	border-radius: .3rem .3rem 0 0;
	padding: .5rem .5rem .5rem;
	border-bottom: 1px solid #eee;
	background-color: #f3f3f3;
}
.tanchu .tanchu-bt {
	border-bottom: 1px solid #0c2e51;
	background-color: #0c2e51;
}
.tanchu-close {
	float: right;
	height: 1.1rem;
	width: 1.1rem;
	padding-top: .2rem;
	margin-top: -.8rem;
	margin-right: -.8rem;
	border-radius: 100%;
	color: #fff;
	background-color: #db4931;
	cursor: pointer;
}
.tanchu3 .tanchu-close, .tanchu1 .tanchu-close {
	margin-top: -.3rem;
	margin-right: -.3rem;
}
a:hover.tanchu-close {
	color:#fff;
}
.tanchu-nr {
	padding: .6rem .7rem;
}
.tanchu-xynr {
	position: absolute;
	left: 0;
	top: 2.1rem;
	right: 0;
	bottom: 2.25rem;
	padding: .6rem;
	line-height: .9rem;
	overflow: hidden;
	overflow-y: auto;
}
.tanchu-xyan {
	position: absolute;
	left: 0;
	right: 0;
	bottom: .25rem;
}
.tanchu-html {
	position: absolute;
	left: .5rem;
	top: .5rem;
	right: .5rem;
	bottom: .5rem;
	border-radius: .4rem;
	box-shadow: 0 0 .25rem #999;
	color: #666;
	background-color: #fff;
}

