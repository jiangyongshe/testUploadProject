	text-align: center;
}
.html-tab02-l {
	float: left;
	width: 100%;
	height: 1rem;
	padding-top: .1rem;
	text-align: center;
}
.html-tab02-r {
	float: left;
	width: 100%;
	height: 1rem;
	padding-top: .1rem;
