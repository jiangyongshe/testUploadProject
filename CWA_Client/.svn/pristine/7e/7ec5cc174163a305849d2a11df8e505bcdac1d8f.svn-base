/*上传按钮*/
.uploader {
	position: relative;
	display: inline-block;
	overflow: hidden;
	width: 5.2rem;
	height: 4rem;
	padding-top: .5rem;
	text-align: center;
	border-radius: .25rem;
	margin-bottom: .25rem;
	color: #ccc;
	border: 1px dashed #ddd;
	background-color: #fff;
}
.filename {
	display: inline-block;
	width: 100%;
	padding-top: .1rem;
	outline: 0 none;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
	font-size: .6rem;
	text-align: center;
	vertical-align: top;
	-moz-box-sizing: border-box;
	-webkit-box-sizing: border-box;
	box-sizing: border-box;
	border: 1px solid #fff;
	color: #ccc;
	background: #fff;
}
.sc-anniu {
	display: none;
}
.uploader input[type=file] {
	position: absolute;
	left: 0;
	top: 0;
	right: 0;
	bottom: 0;
	width: 100%;
	border: 0;
	height: 4rem;
	cursor: pointer;
	filter: alpha(opacity=0);
	-moz-opacity: 0;
	-khtml-opacity: 0;
	opacity: 0;
}
.uploader i {
	display: inline-block;
	padding-top: .2rem;
}


