			$(".orderConfirmBtn").hide();//确认取消按钮
		}
		if(tableHtml!=""){
			$("#orderTitlediv").show();//确认下单弹出框
		}
		//$(".tanchu-html")[0].style.marginTop=-$(".tanchu-html").height()/2+"px";//调整弹出框位置
		return tableHtml;
