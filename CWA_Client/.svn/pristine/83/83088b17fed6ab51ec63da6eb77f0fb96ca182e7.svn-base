.yjmx-list24 {
	width: 26%;
}

/*底部版权*/
footer {
	padding: .6rem 0;
	line-height: 1.1rem;
	text-align: center;
	color: #999;
	font-size: .6rem;
}
footer a {
	padding: 0 .3rem;
}

/*底部固定菜单*/
.footer-nav {
	position: fixed;
	left: 0;
	bottom: 0;
	right: 0;
	z-index: 21;
	height: 3rem;
	text-align: center;
	border-top: 1px solid #18476f;
	box-shadow: 0 -3px 3px rgba(10,39,68,.4);
	color: #2badd7;
    background: -webkit-linear-gradient(#1a4d7a, #152e55); /* Safari 5.1 - 6.0 */
    background: -o-linear-gradient(#1a4d7a, #152e55); /* Opera 11.1 - 12.0 */
    background: -moz-linear-gradient(#1a4d7a, #152e55); /* Firefox 3.6 - 15 */
    background: linear-gradient(#1a4d7a, #152e55); /* 标准的语法 */
}
.footer-nav li {
	display: inline-block;
	vertical-align: middle;
}
.footer-nav li a {
	position: relative;
	display: block;
	padding: .2rem .45rem;
	color: #2badd7;
}
.footer-nav li i {
	color: #2badd7;
	width: 1.6rem;
	height: 1.6rem;
	padding-top: .25rem;
	margin-bottom: .15rem;
	border: 1px solid #2badd7;
	border-radius: 100%;
	font-size: 1rem;
}
.footer-nav li.active a, .footer-nav li.active a i {
	color: #fe8821;
}
.footer-nav li.active a i {
	color: #fe8821;
	border: 1px solid #fe8821;
}
.footer-nav li a em {
	position: absolute;
	right: 0;
	top: 0;
	width: .9rem;
	height: .9rem;
	line-height: .9rem;
	color: #fff;
	border-radius: 100%;
	background-color: #fe8821;
}

@media (min-width:640px) {
  header, .header-nav, .footer-nav, .ggdp-sea, .ggdp-gm {
	width: 640px;
	margin-left: auto;
	margin-right: auto;
  }
}