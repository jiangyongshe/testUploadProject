* {
    -moz-box-sizing: border-box;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
}
html, body {
	overflow-x: hidden;
}
body, div, p, ul, ol, li, textarea, form, h1, h2, h3, h4, h5, dl, dt, dd, img, section, article, aside, header, footer, nav, figure, hgroup {
	margin: 0;
	padding: 0;
}
body {
	font-family: "MicroSoft YaHei", "\5FAE\8F6F\96C5\9ED1", Helvetica, serif;
	min-width: 320px;
	max-width: 640px;
	margin: 0 auto;
	line-height: 18px;
	font-size: 12px;
	-webkit-text-size-adjust: none;
	color: #333;
	word-wrap: break-word;
	word-break: break-all;
	background-color: #fff;
}
input[type="button"], input[type="submit"], input[type="reset"], input[type="text"] {
	-webkit-appearance: none;
}
textarea, button {
	-webkit-appearance: none;
}
select {
	-webkit-appearance: none;
	padding-right: 22px;
	background-image: url(../images/xtb05.png);
	background-position: right center;
	background-repeat: no-repeat;
	font-family: "MicroSoft YaHei", "\5FAE\8F6F\96C5\9ED1", Helvetica, serif;
}
input, textarea, select {
	font-family: "MicroSoft YaHei", Helvetica;
	font-size: 12px;
	vertical-align: middle;
	outline: none;
	margin: 0;
}
header input::-webkit-input-placeholder,
.screen-list input::-webkit-input-placeholder,
.ggtf-sc3 input::-webkit-input-placeholder {
	color: #999;
}
img {
	border: 0;
}
i, em {
	font-style: normal;
}
table {
	border-collapse: collapse;
	border-spacing: 0;
}
ul, ol, li, dl, dd {
	list-style: none;
}
a {
	color: #666;
	text-decoration: none;
}
a:hover {
	color: #666;
	text-decoration: none;
}

