/*按钮动画*/
.btn:hover, .btn:focus {
	-webkit-animation: pulse 1s;
	animation: pulse 1s;
	box-shadow: 0 0 0 1em rgba(255, 255, 255, 0);
}
@-webkit-keyframes pulse {
 0% {
 box-shadow: 0 0 0 0 var(--hover);
}
}
@keyframes pulse {
 0% {
 box-shadow: 0 0 0 0 var(--hover);
}
}
.bg-999 {
    --hover: #eee;
}
.bg-eee {
    --hover: #999;
}
