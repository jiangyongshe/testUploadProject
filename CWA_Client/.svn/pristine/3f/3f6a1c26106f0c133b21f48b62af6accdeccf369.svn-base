}
.yjmx-list li > p {
	float: left;
	padding-left: 6px;
	padding-top: 10px;
	padding-bottom: 10px;
	text-align: center;
}
.yjmx-list01 {
	width: 12%;
}
.yjmx-list02 {
	width: 46%;
}
.yjmx-list03 {
	width: 42%;
}
.yjmx-list04 {
	width: 26%;
}
.yjmx-list22 {
	width: 40%;
}
.yjmx-list23 {
	width: 22%;
}
.yjmx-list24 {
	width: 26%;
}

/*广告查询*/
.ggcx-list {
	clear: both;
}
.ggcx-list li {
	padding: 6px 10px;
	margin-bottom: 4px;
	border-top: 1px solid #2badd7;
	border-bottom: 1px solid #2badd7;
	background-color: #25486d;
}
.ggcx-list li:first-child {
	border-top: 0;
}
.ggcx-tit {
	border-bottom: 1px dashed #2badd7;
}
.ggcx-db {
	position: fixed;
	left: 0;
	right: 0;
	bottom: 50px;
	height: 40px;
	border-top: 1px solid #2badd7;
	background-color: #0c2e51;
}
.ggcx-db.pad-t2 {
  padding-top: 1px;
}
.kefu li {
	padding: 10px 12px;
	margin-top: 10px;
	border-top: 1px solid #2badd7;
	border-bottom: 1px solid #2badd7;
}

/*底部版权*/
footer {
	padding: 12px 0;
	line-height: 22px;
	text-align: center;
	color: #999;
}
footer a {
	padding: 0 6px;
}

/*底部固定菜单*/
.footer-nav {
	position: fixed;
	left: 0;
	bottom: 0;
	right: 0;
	z-index: 21;
	height: 50px;
	text-align: center;
	border-top: 1px solid #18476f;
	box-shadow: 0 -3px 3px rgba(10,39,68,.4);
	color: #2badd7;
    background: -webkit-linear-gradient(#1a4d7a, #152e55); /* Safari 5.1 - 6.0 */
    background: -o-linear-gradient(#1a4d7a, #152e55); /* Opera 11.1 - 12.0 */
    background: -moz-linear-gradient(#1a4d7a, #152e55); /* Firefox 3.6 - 15 */
    background: linear-gradient(#1a4d7a, #152e55); /* 标准的语法 */
}
.footer-nav li {
	display: inline-block;
	vertical-align: middle;
	width: 19%;
}
.footer-nav li a {
	position: relative;
	display: block;
	padding: 4px 9px;
	color: #2badd7;
}
.footer-nav li i {
	color: #2badd7;
	width: 26px;
	height: 26px;
	padding-top: 7px;
	border: 1px solid #2badd7;
	border-radius: 100%;
	font-size: .8rem;
}
.footer-nav li.active a, .footer-nav li.active a i {
	color: #fe8821;
}
.footer-nav li.active a i {
	color: #fe8821;
	border: 1px solid #fe8821;
}
.footer-nav li a em {
	position: absolute;
	right: 14%;
	top: 0;
	width: 18px;
	height: 18px;
	line-height: 18px;
	color: #fff;
	border-radius: 100%;
	background-color: #fe8821;
}

@media (min-width:640px) {
  header, .header-nav, .footer-nav, .ggdp-sea, .ggdp-gm {
	width: 640px;
	margin-left: auto;
	margin-right: auto;
  }
}


@media (min-width:400px) {
  header {
	  padding: 3px 50px 2px 36px;
  }
  .screen-list li {
    padding-left: 78px;
  }
}