/*美化单选和复选框*/
.checkbox, .radio {
	cursor: pointer;
}
.checkbox i {
	font-style: normal;
	display: inline-block;
	width: 22px;
	height: 22px;
	text-align: center;
	line-height: 22px;
	color: #fff;
	vertical-align: middle;
	margin: -2px 2px 1px 0;
	border: 1px solid #ccc;
	background: #fff;
}
.radio i {
	font-style: normal;
	display: inline-block;
	width: 22px;
	height: 22px;
	text-align: center;
	line-height: 22px;
	color: #fff;
	vertical-align: middle;
	margin: -2px 2px 1px 0;
	border: 1px solid #ccc;
	background: #fff;
}
.h-login .radio i {
	color: #333;
	border: 2px solid #fff;
	background-color: #333;
}
input[type="checkbox"], input[type="radio"] {
	display: none;
}
input[type="radio"] + i {
	border-radius: 100%;
}
input[type="checkbox"]:checked + i, input[type="radio"]:checked + i {
	color: #fff;
	border: 2px solid #d9140f;
	background-color: #d9140f;
}
.h-login input[type="checkbox"]:checked + i, .h-login input[type="radio"]:checked + i {
	color: #ff0;
	border: 2px solid #fff;
}
input[type="checkbox"]:disabled + i, input[type="radio"]:disabled + i {
	border-color: #999;
}
input[type="checkbox"]:checked:disabled + i, input[type="radio"]:checked:disabled + i {
	background: #999;
}

