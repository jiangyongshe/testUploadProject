@charset "utf-8";
/* CSS Document */
* {
    -moz-box-sizing: border-box;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
}
html,body,div,p,ul,ol,li,textarea,form,h1,h2,h3,h4,h5,dl,dt,dd,img,section,article,aside,header,footer,menu,nav,figure,hgroup {
	margin: 0;
	padding: 0;
}
body {
	font-family: "MicroSoft YaHei","\5FAE\8F6F\96C5\9ED1",Helvetica,serif;
	-webkit-text-size-adjust: none;
	min-width: 320px;
	font-size: 12px;
	line-height: 18px;
	word-wrap: break-word;
	word-break: break-all;
	color: #666;
	background-color: #fff;
}
input[type="button"],
input[type="submit"],
input[type="reset"] {
    -webkit-appearance: none;
}
textarea {
    -webkit-appearance: none;
}
input,textarea {
	font: 14px/1.5 "MicroSoft YaHei",Helvetica;
	vertical-align: middle;
	outline: none;
}
img {
	border: 0;
}
i,em {
	font-style: normal;
}
table {
	border-collapse: collapse;
	border-spacing: 0;
}
ul,ol,li,dl,dd {
	list-style: none;
}
a {
	color: #666;
	text-decoration: none;
}
a:hover {
	color: #d9140f;
	text-decoration: none;
}

/*头部*/
header {
	width: 1200px;
	margin: 0 auto;
}
.logo {
	display: inline-block;
	vertical-align: middle;
	width: 14%;
	padding: 12px 0;
}
.logo img {
	float: left;
	width: 100%;
}
menu {
	display: inline-block;
	vertical-align: middle;
	font-size: 16px;
	padding-left: 80px;
}
menu li {
	float: left;
	padding-right: 40px;
}
menu li a {
	display: block;
	padding-bottom: 8px;
}
menu li.active a, menu li a:hover {
	color: #d9140f;
	border-bottom: 3px solid #d9140f;
}
menu li a:hover {
	animation: xiant 2s;
}
@keyframes xiant
{
from { border-bottom-color: rgba(217,20,15,0);}
to { border-bottom-color: rgba(217,20,15,1);}
}
.hear-r {
	padding-top: 16px;
	padding-bottom: 8px;
}
.hear-r > span a {
	padding: 0 15px;
}

/*底部*/
footer {
	padding: 20px 0;
	line-height: 24px;
	text-align: center;
	color: #999;
	background-color: #333;
}
footer a {
	padding: 0 15px;
	color: #fff;
}

/*表格样式*/
.rmgg-table {
	  clear: both;
	  width: 100%;
	  overflow-x: auto;
	  overflow-y: hidden;
    }
.rmgg-table th, .rmgg-table td {
