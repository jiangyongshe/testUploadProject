package com.cwa.client.dto;

import java.sql.Timestamp;

public class AdvertiserDeviceDto extends BaseDto{

	private Integer id;
	private Integer advertiser_id;//广告商id
	private String device_id;// 设备id
	private Integer device_use_type;// 0:未定1：买断 2：租用3：自用
	private Integer device_type;// 设备类型(0：未知，1=类型1、2=类型2)
	private Integer device_status;// 设备状态（1：正常，2：安装中，3：异常，4：休眠，5：关闭，6：维修中）
	private String device_detail;// 设备详情
	private String commit_id;// 创建人
	private Timestamp commit_time;// 创建时间
	private String device_address;// 设备地址
	private String begin_time;// 视频播放开始时间
	private String end_time;// 视频播放结束时间
	
	public Integer getId() {
		return id;
	}
	public void setId(Integer id) {
		this.id = id;
	}
	public Integer getAdvertiser_id() {
		return advertiser_id;
	}
	public void setAdvertiser_id(Integer advertiser_id) {
		this.advertiser_id = advertiser_id;
	}
	public String getDevice_id() {
		return device_id;
	}
	public void setDevice_id(String device_id) {
		this.device_id = device_id;
	}
	public Integer getDevice_use_type() {
		return device_use_type;
	}
	public void setDevice_use_type(Integer device_use_type) {
		this.device_use_type = device_use_type;
	}
	public Integer getDevice_type() {
		return device_type;
	}
	public void setDevice_type(Integer device_type) {
		this.device_type = device_type;
	}
	public Integer getDevice_status() {
		return device_status;
	}
	public void setDevice_status(Integer device_status) {
		this.device_status = device_status;
	}
	public String getDevice_detail() {
		return device_detail;
	}
	public void setDevice_detail(String device_detail) {
		this.device_detail = device_detail;
	}
	public String getCommit_id() {
		return commit_id;
	}
	public void setCommit_id(String commit_id) {
		this.commit_id = commit_id;
	}
	public Timestamp getCommit_time() {
		return commit_time;
	}
	public void setCommit_time(Timestamp commit_time) {
		this.commit_time = commit_time;
	}
	public String getDevice_address() {
		return device_address;
	}
	public void setDevice_address(String device_address) {
		this.device_address = device_address;
	}
	public String getBegin_time() {
		return begin_time;
	}
	public void setBegin_time(String begin_time) {
		this.begin_time = begin_time;
	}
	public String getEnd_time() {
		return end_time;
	}
	public void setEnd_time(String end_time) {
		this.end_time = end_time;
	}
}
