@charset "utf-8";
/* CSS Document */
* {
    -moz-box-sizing: border-box;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
}
html,body,div,p,ul,ol,li,textarea,form,h1,h2,h3,h4,h5,dl,dt,dd,img,section,article,aside,header,footer,menu,nav,figure,hgroup {
	margin: 0;
	padding: 0;
}
body {
	font-family: "MicroSoft YaHei","\5FAE\8F6F\96C5\9ED1",Helvetica,serif;
	-webkit-text-size-adjust: none;
	min-width: 320px;
	font-size: 12px;
	line-height: 18px;
	word-wrap: break-word;
	word-break: break-all;
	color: #666;
	background-color: #fff;
}
input[type="button"],
input[type="submit"],
input[type="reset"] {
    -webkit-appearance: none;
}
textarea {
    -webkit-appearance: none;
}
input,textarea {
	font: 14px/1.5 "MicroSoft YaHei",Helvetica;
	vertical-align: middle;
	outline: none;
}
img {
	border: 0;
}
i,em {
	font-style: normal;
}
table {
	border-collapse: collapse;
	border-spacing: 0;
}
ul,ol,li,dl,dd {
	list-style: none;
}
a {
	color: #666;
	text-decoration: none;
}
a:hover {
	color: #e10031;
	text-decoration: none;
}

/*顶部*/
.ding {
	float: left;
	width: 100%;
	padding: 7px 0;
	color: #999;
	border-bottom: 1px solid #eee;
	background-color: #f3f3f3;
}
.ding a {
	color: #999;
}
.ding a.red {
	color: #e10031;
}
.ding a:hover {
	color: #e10031;
	text-decoration: underline;
}
.ding-r > a, .glzx {
	display: inline-block;
	padding-left: 8px;
	padding-right: 8px;
	border-right: 1px solid #ccc;
}
.ding-r a:last-child {
	border-right: 0;
	padding-right: 0;
}
.glzx:hover {
	color: #e10031;
	cursor: pointer;
}
.glzx-menu {
	left: 0;
	right: 0;
	top: 22px;
	z-index: 1;
    border: 1px solid #ddd;
    border-radius: 5px;
	background: #f9f9f9;
	display: none;
}
.glzx:hover .glzx-menu {
	display: block;
}
.glzx-menu:after {
	content: "";
	position: absolute;
	left: 50%;
	top: -7px;
	margin-left: -8px;
	border-left: 8px solid transparent;
	border-right: 8px solid transparent;
	border-bottom: 8px solid #f9f9f9;
}
.glzx-menu:before {
	content: "";
	position: absolute;
	left: 50%;
	top: -8px;
	margin-left: -8px;
	border-left: 8px solid transparent;
	border-right: 8px solid transparent;
	border-bottom: 8px solid #ddd;
}
.glzx-menu a {
	display: block;
	padding: 6px 0 6px 10px;
}

/*头部*/
header {
	width: 1200px;
	margin: 0 auto;
}
.logo {
	width: 18%;
	padding: 20px 0;
}
.logo img {
	float: left;
	width: 100%;
}
.home-search {
	padding-top: 30px;
	padding-right: 220px;
}
.hear-r {
	right: 0;
	top: 18px;
}
.hear-r img {
	width: 200px;
}
menu {
	float: left;
	width: 100%;
	font-size: 16px;
	border-bottom: 3px solid #127cce;
}
menu li {
	display: inline-block;
	vertical-align: bottom;
	padding-right: 10px;
}
menu li a {
	display: block;
	padding: 11px 35px 8px;
}
menu li.active a, menu li a:hover {
	color: #fff;
	border-radius: 5px 5px 0 0;
	background-color: #127cce;
}

/*底部*/
footer {
	float: left;
	width: 100%;
	padding: 20px 0;
	line-height: 24px;
	text-align: center;
	color: #999;
	background-color: #333;
}
footer a {
	padding: 0 15px;
	color: #fff;
}

/*表格样式*/
.rmgg-table {
	  clear: both;
	  width: 100%;
    }
.rmgg-table th, .rmgg-table td {
