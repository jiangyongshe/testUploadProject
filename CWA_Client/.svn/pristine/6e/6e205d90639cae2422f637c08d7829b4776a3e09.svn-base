/*店铺详情*/
.data-list-b .dian-xq {
	display: none;
}
.data-list-b:hover .dian-xq {
	display: block;
}
.dian-xq {
	position: absolute;
	top: -30px;
	left: 50%;
	z-index: 1;
	width: 616px;
	padding: 12px 2px 12px 12px;
	text-align: left;
	line-height: 24px;
	border: 1px solid #ccc;
	border-radius: 5px;
	background-color: rgba(255,255,255,1);
}
.dian-xq:after {
	content: "";
	position: absolute;
	left: -7px;
	top: 50px;
	border-top: 8px solid transparent;
	border-bottom: 8px solid transparent;
	border-right: 8px solid #fff;
}
.dian-xq:before {
	content: "";
	position: absolute;
	left: -8px;
	top: 50px;
	border-top: 8px solid transparent;
	border-bottom: 8px solid transparent;
	border-right: 8px solid #ccc;
}
.dian-bt {
	cursor: pointer;
}
.dian-close {
	position: fixed;
	right: 5px;
	top: 5px;
	display: block;
	width: 24px;
	height: 24px;
	color: #fff;
	border-radius: 100%;
	background-color: #127cce;
	cursor: pointer;
}
.dian-close:hover {
	color: #fff;
}
.dian-xq-img img {
	float: left;
	width: 120px;
	padding-right: 10px;
	padding-top: 10px;
	vertical-align: top;
}

