<body class="body1">
<script src="/chinese/mobile/js/header.js"></script><!--公用头部JS-->

<div class="font12">
  <ul class="ggtf-sc ggtf-sc2 pad-q10 bor-b4 col-fff">
        <li>
          <span class="pos-a ggtf-sc-bt" style="top:0;">提现方式</span>
          <p class="payType">
            <label class="radio"><input type="radio" value="0101" name="radio" id="" checked ><i>✓</i> 微信</label>
            <label class="radio pad-l10"><input type="radio" value="0201" name="radio" id="" ><i>✓</i> 支付宝</label>
          </p>
        </li>
  </ul>
  
  <div class="ggtf-sc ggtf-sc3 pad-q10 bor-t4 bor-b4 mar-t8" style="padding-top:0;">
        <ul class="">
          <li>
            <span class="pos-a ggtf-sc-bt">微信账号</span>
            <input type="text" class="wenb03 width100" placeholder="请输入微信账号" />
          </li>
        
          <li>
            <span class="pos-a ggtf-sc-bt">真实姓名</span>
            <input type="text" class="wenb03 width100" placeholder="请输入真实姓名" />
          </li>
          
          <li>
            <span class="pos-a ggtf-sc-bt">提现金额</span>
            <input type="text" class="wenb03 width100 amount" placeholder="请输入提现金额" />
          </li>
        </ul>
        
        <ul class="hide">
          <li>
            <span class="pos-a ggtf-sc-bt">支付宝账号</span>
            <input type="text" class="wenb03 width100" placeholder="请输入微信账号" />
          </li>
        
          <li>
            <span class="pos-a ggtf-sc-bt">真实姓名</span>
            <input type="text" class="wenb03 width100" placeholder="请输入真实姓名" />
          </li>
          
          <li>
            <span class="pos-a ggtf-sc-bt">提现金额</span>
            <input type="text" class="wenb03 width100 amount" placeholder="请输入充值金额" />
          </li>
        </ul>
  </div>
  
  <input name="" id="commit" type="button" value="提交" class="btn btn11 bg-blue width100 pad-q10 font13 mar-t20" />
</div>
<script src="/chinese/mobile/js/globalVar.js"></script>
<script src="/chinese/mobile/js/common.js"></script>
<script src="/chinese/mobile/js/footer.js"></script><!--公用底部JS-->
