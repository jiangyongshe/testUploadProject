package com.cwa.client.dto;

public class BaseDto {

	private Integer pageSize;
	private Integer pageNo;
	private Integer rowNo;//行号
	
	private String selectField;// 自定义查询字段

	private String startTime;// 开始时间
	
	private String endTime;// 结束时间
	
	private String timeField;// 时间字段
	
	private String timeFormat = "%Y-%m-%d %H:%i:%s";// 默认时间格式
	
	private String orderWay = "ORDER BY A.ID DESC ";// 默认排序方式
	
	private String whereCondition;// 自定义查询条件
	
	private String relevanceCondition;// 关联条件
	
	public Integer getPageSize() {
		return pageSize;
	}
	public void setPageSize(Integer pageSize) {
		this.pageSize = pageSize;
	}
	public Integer getPageNo() {
		return pageNo;
	}
	public void setPageNo(Integer pageNo) {
		this.pageNo = pageNo;
	}
	public Integer getRowNo() {
		return rowNo;
	}
	public void setRowNo(Integer rowNo) {
		this.rowNo = rowNo;
	}
	public String getSelectField() {
		return selectField;
	}
	public void setSelectField(String selectField) {
		this.selectField = selectField;
	}
	public String getStartTime() {
		return startTime;
	}
	public void setStartTime(String startTime) {
		this.startTime = startTime;
	}
	public String getEndTime() {
		return endTime;
	}
	public void setEndTime(String endTime) {
		this.endTime = endTime;
	}
	public String getTimeField() {
		return timeField;
	}
	public void setTimeField(String timeField) {
		this.timeField = timeField;
	}
	public String getTimeFormat() {
		return timeFormat;
	}
	public void setTimeFormat(String timeFormat) {
		this.timeFormat = timeFormat;
	}
	public String getOrderWay() {
		return orderWay;
	}
	public void setOrderWay(String orderWay) {
		this.orderWay = orderWay;
	}
	public String getWhereCondition() {
		return whereCondition;
	}
	public void setWhereCondition(String whereCondition) {
		this.whereCondition = whereCondition;
	}
	public String getRelevanceCondition() {
		return relevanceCondition;
	}
	public void setRelevanceCondition(String relevanceCondition) {
		this.relevanceCondition = relevanceCondition;
	}
	
}
