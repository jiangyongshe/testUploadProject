/* 焦点图 */
.slide-main {
	height: 330px;
	position: relative;
}
.prev,.next {
	display: block;
	width: 44px;
	height: 44px;
	position: absolute;
	z-index: 222;
	top: 140px;
	overflow: hidden;
	cursor: pointer;
	opacity: 0.6;
	-moz-opacity: 0.6;
	filter: alpha(opacity=60);
}
.prev {
	left: 20px;
}
.next {
	right: 20px;
}
.prev:hover,.next:hover,.nav-main a:hover,.nav-main a.cur {
	opacity: 1;
	-moz-opacity: 1;
	filter: alpha(opacity=100);
}
.item {
	display: block;
	width: 100%;
	height: 12px;
	position: absolute;
	z-index: 9;
	bottom: 15px;
	left: 0;
	text-align: center;
}
.item a {
	display: inline-block;
	width: 10px;
	height: 10px;
	margin-right: 11px;
	background: #fff;
	opacity: 0.5;
	-moz-opacity: 0.5;
	filter: alpha(opacity=50);
	border-radius: 100%;
	overflow: hidden;
}
.item a.cur {
	width: 22px;
	border-radius: 20px;
	opacity: 1;
	-moz-opacity: 1;
	filter: alpha(opacity=1);
	background-color: #e10031;
}
.slide-box,.slide {
	display: block;
	width: 100%;
	height: 330px;
	overflow: hidden;
}
.slide-box {
	position: relative;
}
.slide {
	display: none;
	background: url(../images/banner01.jpg) center top no-repeat;
	position: relative;
	position: absolute;
	z-index: 8;
}
#bgstylea {
	background: url(../images/banner03.jpg) center top no-repeat;
	background-size: 100%;
}
#bgstyleb {
	background: url(../images/banner02.jpg) center top no-repeat;
	background-size: 100%;
}
#bgstylec {
	background: url(../images/banner01.jpg) center top no-repeat;
	background-size: 100%;
}
.slide a {
	display: block;
	width: 100%;
	cursor: pointer;
}

