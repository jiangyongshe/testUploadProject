/*正在咨询、求购信息、成交店铺*/
.cjzx, .cjdz, .qgxx {
    width: 390px;
	padding: 6px 18px 18px;
	border-radius: 0 0 5px 5px;
    background-color: #fff;
}
.cjzx-bt > span {
	display: inline-block;
    position: relative;
    top: -3px;
    width: 140px;
    margin-top: -9px;
	padding: 8px 0;
    font-size: 18px;
	font-weight: normal;
	color: #fff;
}
.cjzx-bt > span::before {
	content: "";
	position: absolute;
	left: -6px;
	top: 0;
	border-bottom: 6px solid #841e1e;
    border-left: 6px solid transparent;
}
.cjzx-bt > span::after {
	content: "";
	position: absolute;
	right: -6px;
	top: 0;
	border-bottom: 6px solid #841e1e;
    border-right: 6px solid transparent;
}
.cjzx {
	border-top: 1px solid #af1e23;
}
.cjzx .cjzx-bt > span {
	background-color: #af1e23;
}
