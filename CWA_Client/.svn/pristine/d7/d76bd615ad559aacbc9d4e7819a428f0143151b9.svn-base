/*遮罩*/
.rmgg li .a2 {
	position: absolute;
	top: 0;
	left: -100%;
	bottom: 0;
	width: 100%;
	color: #fff;
	background: rgba(0,0,0,0.7);
	transition: all 0.2s;
	overflow: hidden;
} 
.rmgg li .a2 .p4 {
	width: 100%;
	padding-left: 10px;
	padding-right: 10px;
	position: absolute;
	top: 38px;
	left: -100%;
	color: #ffe605;
    font-weight: bold;
    transition: all 0.2s 0.1s ease-out ;  
}
.rmgg li .a2 .p5 {
	width: 100%;
	position: absolute;
	top: 58px;
	left: -100%;
    transition: all 0.2s 0.2s ease-out ;  
}
.rmgg li .a2 .p6 {
	position: absolute;
	top: 85px;
	left: 0;
	width: 100%;
	transform: scale(1.4);
	transition: all 0.3s 0.2s ;
}
.rmgg li .a2 .p6 > span {
	padding-right: 10px;
	line-height: 24px;
}
.rmgg li .a2 .p7 {
	position: absolute;
	bottom: -74px;
	width: 100%;
	transition: all 0.2s 0.5s ;
}
.rmgg li .a2 .p7 img {
	float: left;
	width: 57px;
	max-height: 57px;
	padding-right: 2px;
	overflow: hidden;
}

