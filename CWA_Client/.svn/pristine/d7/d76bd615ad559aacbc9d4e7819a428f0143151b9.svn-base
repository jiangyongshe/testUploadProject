.h-login {
	right: 0;
	top: 110px;
	bottom: 110px;
	z-index: 220;
	width: 300px;
	padding: 30px 25px 0;
	color: #fff;
	border-radius: 5px;
	background-color: rgba(0,0,0,.6);
}
.h-login:after {
	content: "";
	position: absolute;
	right: 0;
	top: 0;
	border-right: 20px solid rgba(217,20,15,.7);
	border-bottom: 20px solid transparent;
	z-index: 2;
}
.h-login-bt {
	width: 100%;
}
.h-login-tb {
	left: 0;
	top: 22px;
	width: 40px;
	height: 40px;
	line-height: 40px;
	text-align: center;
	color: #999;
}
.h-login .h-login-b a {
	display: inline-block;
	padding: 10px 8px;
	color: #fff;
}

/* 焦点图 */
.slide-main {
	height: 330px;
	position: relative;
}
.prev,.next {
	display: block;
	width: 44px;
	height: 44px;
	position: absolute;
	z-index: 222;
	top: 140px;
	overflow: hidden;
	cursor: pointer;
	opacity: 0.6;
	-moz-opacity: 0.6;
	filter: alpha(opacity=60);
}
.prev {
	left: 20px;
}
.next {
	right: 20px;
}
.prev:hover,.next:hover,.nav-main a:hover,.nav-main a.cur {
	opacity: 1;
	-moz-opacity: 1;
	filter: alpha(opacity=100);
}
.item {
	display: block;
	width: 100%;
	height: 12px;
	position: absolute;
	z-index: 9;
	bottom: 15px;
	left: 0;
	text-align: center;
}
.item a {
	display: inline-block;
	width: 10px;
	height: 10px;
	margin-right: 11px;
	background: #fff;
	opacity: 0.5;
	-moz-opacity: 0.5;
	filter: alpha(opacity=50);
	border-radius: 100%;
	overflow: hidden;
}
.item a.cur {
	width: 22px;
	border-radius: 20px;
	opacity: 1;
	-moz-opacity: 1;
	filter: alpha(opacity=1);
	background-color: #f22e00;
}
.slide-box,.slide {
	display: block;
	width: 100%;
	height: 330px;
	overflow: hidden;
}
.slide-box {
	position: relative;
}
.slide {
	display: none;
	height: 330px;
	background: url(../images/banner01.jpg) center top no-repeat;
	position: relative;
	position: absolute;
	z-index: 8;
}
#bgstylea {
	background: url(../images/banner03.jpg) center top no-repeat;
	background-size: 100%;
}
#bgstyleb {
	background: url(../images/banner02.jpg) center top no-repeat;
	background-size: 100%;
}
#bgstylec {
	background: url(../images/banner01.jpg) center top no-repeat;
	background-size: 100%;
}
.slide a {
	display: block;
	width: 100%;
	height: 330px;
	cursor: pointer;
}

