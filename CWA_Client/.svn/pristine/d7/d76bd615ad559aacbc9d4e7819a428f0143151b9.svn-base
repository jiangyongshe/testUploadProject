/*正在咨询、求购信息、成交店铺*/
.cjzx, .cjdz, .qgxx {
    width: 390px;
	padding: 6px 18px 18px;
	border-radius: 0 0 5px 5px;
    background-color: #fff;
}
.cjzx {
	border-top: 1px solid #f22e00;
}
.cjdz {
	margin-left: 15px;
	border-top: 1px solid #ff7c1c;
}
.qgxx {
	border-top: 1px solid #ffa524;
}
.cjzx-bt > span {
	display: inline-block;
    position: relative;
    top: -3px;
    width: 140px;
    margin-top: -9px;
	padding: 8px 0;
    font-size: 18px;
	font-weight: normal;
	color: #fff;
	background-color: #f22e00;
}
.cjzx-bt > span::before {
	content: "";
	position: absolute;
	left: -6px;
	top: 0;
	border-bottom: 6px solid #c84e4e;
    border-left: 6px solid transparent;
}
.cjzx-bt > span::after {
	content: "";
	position: absolute;
	right: -6px;
	top: 0;
	border-bottom: 6px solid #c84e4e;
    border-right: 6px solid transparent;
}
.cjdz .cjzx-bt > span {
	background-color: #ff7c1c;
}
.qgxx .cjzx-bt > span {
	background-color: #ffa524;
}
.cjzx-list, .ggdp-list {
	min-height: 300px;
}
.cjzx-list li {
	padding-top: 12px;
	height: 30px;
	white-space: nowrap;
	text-overflow:ellipsis;
	-o-text-overflow: ellipsis;
	overflow: hidden;
}
.qgxx .cjzx-list li a {
	display: inline-block;
	width: 270px;
	white-space: nowrap;
	text-overflow:ellipsis;
	-o-text-overflow: ellipsis;
	overflow: hidden;
}
.ggdp-list li {
	padding-top: 8px;
	padding-bottom: 8px;
	border-bottom: 1px solid #eee;
}
.ggdp-list-xq > span {
	padding-left: 3px;
	margin-left: -3px;
	height: 18px;
	white-space: nowrap;
	text-overflow:ellipsis;
	-o-text-overflow: ellipsis;
	overflow: hidden;
}

