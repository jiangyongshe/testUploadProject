       </property>
       <property name="accessKeySecret">
          <value>9eupK3x6EmlE4QIsJob5Em6vKjQSo9</value>
       </property>
       <property name="signName">
          <value>翔云播</value>
       </property>
          <property name="connectTimeOut">
