/* 焦点图 */
.slide-main {
	height: 360px;
	position: relative;
}
.prev,
.next {
	display: block;
	width: 44px;
	height: 44px;
	position: absolute;
	z-index: 222;
	top: 50%;
	margin-top: -22px;
	overflow: hidden;
	cursor: pointer;
	opacity: 0.6;
	-moz-opacity: 0.6;
	filter: alpha(opacity=60);
}
.prev {
	left: 20px;
}
.next {
	right: 20px;
}
.prev:hover,
.next:hover,
.nav-main a:hover,
.nav-main a.cur {
	opacity: 1;
	-moz-opacity: 1;
	filter: alpha(opacity=100);
}
.item {
	display: block;
	width: 100%;
	height: 12px;
	position: absolute;
	z-index: 9;
	bottom: 15px;
	left: 0;
	text-align: center;
}
.item a {
	display: inline-block;
	width: 10px;
	height: 10px;
	margin-right: 11px;
	background: #fff;
	opacity: 0.5;
	-moz-opacity: 0.5;
	filter: alpha(opacity=50);
	border-radius: 100%;
	overflow: hidden;
}
.item a.cur {
	width: 22px;
	border-radius: 20px;
	opacity: 1;
	-moz-opacity: 1;
	filter: alpha(opacity=1);
	background-color: #fff;
}
.slide-box, 
.slide {
	display: block;
	width: 100%;
	height: 360px;
	overflow: hidden;
}
.slide-box {
	position: relative;
}
.slide {
	display: none;
	position: absolute;
	z-index: 8;
	background-position: center top;
	background-repeat: no-repeat;
	background-size: cover;
}
.slide a {
	display: block;
	width: 100%;
	cursor: pointer;
}

